<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

  <module name="gst" package="com.axelor.gst.db" />

  <entity name="Sequence">
    <string name="prefix" title="prifix" required="true"></string>
    <string name="suffix" title="Suffix"></string>
    <integer name="padding" title="Padding" min="1" max="10"></integer>
    <string name="nextNumber" title="Next Number" formula="true" large="true" multiline="true"></string>
    <many-to-one name="model" ref="com.axelor.meta.db.MetaModel" title="Model"></many-to-one>
  </entity>

</domain-models>
