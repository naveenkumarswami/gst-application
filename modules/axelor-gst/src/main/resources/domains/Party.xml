<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

  <module name="gst" package="com.axelor.gst.db"/>
  
  <entity name="Party">
  <string name="reference" title="Reference" sequence="company.seq"></string>
  <string name="name" title="Name"></string>
  <string name="type" title="Type" selection="party.state.selection" default="Company"></string>
  <boolean name="iscustomer" title="customber" ></boolean>
  <boolean name="issupplier" title="supplier"></boolean>
  <string name="gstin" min="15" max="15"></string>
  <one-to-many name="contact" ref="Contact"/>
  <one-to-many name="address" ref="Address"/>
  </entity>

</domain-models>
