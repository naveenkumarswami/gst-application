            <xml-property name="queryText"><![CDATA[select invoice.datet as invoiceDate , 
invoice.reference , 

invoiceAdd.line1 as invoiceLine1 ,
invoiceAdd.line2 as invoiceLine2, invoiceAdd.pin_code as invoicePinCode ,
city.name as invoiceCity , state.name as invoiceState, 
country.name as invoiceCountry ,  

shippingAdd.line1 as shppingLine1 ,
shippingAdd.line2 as shppingLine2, 
shippingAdd.pin_code as shppingPinCode ,
shippingCity.name shpping_city , shippingState.name as shpping_state, 
shippingCountry.name as shpping_country ,

invoice.net_amount , invoice.net_csgt ,
invoice.net_igst , invoice.net_sgst , invoice.gross_amount ,
party.name from gst_invoice as invoice 

left join gst_party as party on invoice.party = party.id
left join gst_address as invoiceAdd on invoice.invoice_address = invoiceAdd.id
left join gst_city as city on invoiceAdd.city = city.id
left join gst_state as state on invoiceAdd.state = state.id
left join gst_country as country on invoiceAdd.country = country.id

left join gst_address as shippingAdd on invoice.shipping_address = shippingAdd.id
left join gst_city as shippingCity on shippingAdd.city = shippingCity.id 
left join gst_state as shippingState on shippingAdd.state = shippingState.id
left join gst_country as shippingCountry on shippingAdd.country = shippingCountry.id

where invoice.id =?;]]></xml-property>
