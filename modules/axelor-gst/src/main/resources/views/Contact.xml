<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">

  <grid model="com.axelor.gst.db.Contact" title="Contact" name="contact-grid">
    <field name="name"></field>
    <field name="type"></field>
    <field name="primaryEmail"></field>
    <field name="secondaryPhone"></field>
  </grid>

  <form model="com.axelor.gst.db.Contact" title="Contact" name="contact-form">
    <panel title="Contact">
      <field name="name"></field>
      <field name="type"></field>
      <field name="primaryEmail"></field>
      <field name="secondaryEmail"></field>
       <field name="primaryPhone"></field>
      <field name="secondaryPhone"></field>
    </panel>
  </form>

</object-views>
  