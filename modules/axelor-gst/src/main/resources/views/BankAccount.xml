<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">

  <grid name="bankAccount-grid" title="Bank Account List" model="com.axelor.gst.db.BankAccount">
    <field name="bank" />
    <field name="accountNo"></field>
    <field name="ifsc"></field>
    <field name="type"></field>
    <field name="address"></field>
  </grid>

  <form name="bankAccount-form" title="Bank Account" cols="2" model="com.axelor.gst.db.BankAccount">
    <panel title="Overview">
      <field name="bank" />
      <field name="accountNo"></field>
      <field name="ifsc"></field>
      <field name="type"></field>
      <field name="address"></field>
    </panel>
  </form>

</object-views>
