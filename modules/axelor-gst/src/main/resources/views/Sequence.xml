<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">

  <grid name="sequence-grid" title="sequence List" model="com.axelor.gst.db.Sequence">
    <field name="prefix" />
    <field name="suffix"></field>
    <field name="padding" />
    <field name="nextNumber"></field>
  </grid>

  <form name="sequence-form" title="Sequence" cols="2" model="com.axelor.gst.db.Sequence" onSave="action-sequence-genrate-auto">
    <panel title="Overview">
      <field name="prefix" />
      <field name="suffix"></field>
      <field name="padding"></field>
      <field name="nextNumber" />
      <field name="model" form-view="meta-model-form" grid-view="meta-model-grid" />
    </panel>
  </form>
  
   <action-method name="action-sequence-genrate-auto">
   <call class="com.axelor.gst.web.GstController" method="getSequence"/>
   </action-method>

</object-views>
