<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">

  <grid name="productCategory-grid" title="Product Category List" model="com.axelor.gst.db.ProductCategory">
    <field name="name"/>
    <field name="code"></field>
    <field name="gstRate"></field>
  </grid>

  <form name="productCategory-form" title="Product Category" cols="2" model="com.axelor.gst.db.ProductCategory">
    <panel title="Products">
      <field name="name" />
      <field name="code"></field>
      <field name="gstRate"></field>
    </panel>
  </form>

</object-views>
